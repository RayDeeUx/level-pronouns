{
	"geode": "4.0.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"tags": [
		"joke"
	],
	"id": "n.level-pronouns",
	"name": "Level Pronouns",
	"version": "v1.1.3",
	"developer": "n",
	"description": "Enhance 2.2 with pronouns on levels!",
	"repository": "https://github.com/NicknameGG/level-pronouns",
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.18.0",
			"importance": "required"
		}
	],
	"incompatibilities": [
		{
			"id": "n.level_pronouns",
			"version": "*",
			"importance": "superseded"
		}
	],
	"settings": {
		"neopronouns": {
			"name": "Use Neopronouns",
			"description": "Use neopronouns in levels. <cy>Warning</c>: Disabling will make levels with neopronouns have normal pronouns.",
			"type": "bool",
			"default": true
		},
		"White Text": {
			"type": "bool",
			"name": "White Text",
			"description": "Use black text for pronouns. In case you have dark mode mod.",
			"default": false
		}
	}
}
